journal = {BMC health services research},
keywords = {Adult,Aged,Catchment Area (Health),Data Collection,Empirical Research,Family Characteristics,Female,Financing, Personal,Financing, Personal: statistics {\&} numerical data,Health Care Surveys,Health Expenditures,Health Expenditures: statistics {\&} numerical data,Humans,Income,Male,Middle Aged,National Health Programs,National Health Programs: standards,National Health Programs: utilization,Regression Analysis,Socioeconomic Factors,Taiwan},
month = {jan},
number = {1},
pages = {60},
pmid = {16137336},
title = {{Household out-of-pocket medical expenditures and National Health Insurance in Taiwan: income and regional inequality.}},
