@article{Harris2001,
abstract = {For many years now, farmers have been criticised for their methods of production. It is popularly believed that the intensive use of chemicals, in particular, can lead to a degree of environmental pollution too great to ignore. The recent crises in animal husbandry have done nothing to take the sting out of this criticism, An improvement in agricultural policy is being demanded which would bring farming closer to nature, and provide for improvements in animal welfare and consumer-orientation. The Federal Government aims to intensify its promotion of ecological farming, ra ising the question of how this will affect the conventional farmers, who are still farming over 90 {\%} of our arable land today. A number of surveys in recent years have shown that the majority of conventional farmers try their best to keep the interests of environmental protection in view. In this article, too, where we are addressing the subject of pesticide management, such efforts on the part of farmers are plain for all to see. For this survey, 74 farmers (mainly from full-time cash-crop farms) were selected from 73 parishes evenly spread over the State of Baden-Wurttemberg. All the respondent farmers held the necessary technical qualifications (certificate of technical competence) and possessed field-crop sprayers with valid inspection plates. In the way of a check, 73 {\%} of the respondents arranged for a "spraying window", and 4/5 kept plot records of the measures they had taken to protect their plants. All this shows that farmers are able to manage their pesticides in a manner compatible with the environment. It is also clear that farmers adopt differentiated strategies for their pesticide management, acting prophylactically after the first signs of infestation, and in accordance with damage-threshold principles. None of these strategies predominates. The results of the survey clearly reveal what farmers believe is delaying the application of the damage-threshold principle to pesticide management. The survey suggests that the "conventional" farmers interviewed are well on the way to developing sound practical methods, and that they have therefore earned the same support as their "ecological" colleagues.},
annote = {494XX
Times Cited:0
Cited References Count:8},
author = {Harris, J},
isbn = {0005-9080},
journal = {Berichte Uber Landwirtschaft},
language = {German},
number = {4},
pages = {565--577},
title = {{Efforts to find environmentally compatible methods of pesticide management - a survey of 74 farmers in Baden-Wurttemberg}},
volume = {79},
year = {2001}
}
