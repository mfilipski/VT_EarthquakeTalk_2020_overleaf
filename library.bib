keywords = {Euro,elasticity,household labor supply,taxation},
number = {5820},
title = {{Labor Supply Elasticities in Europe and the US}},
url = {http://ideas.repec.org/p/iza/izadps/dp5820.html{\%}5Cnhttp://ftp.iza.org/dp5820.pdf},
year = {2011}
}
