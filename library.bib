@article{Straubhaar1986,
abstract = {Zusammenfassung  Die Bestimmungsgr{\"{u}}nde f{\"{u}}r die Heimat{\"{u}}berweisungen der Gastarbeiter: Der Fall der T{\"{u}}rkei. - Dieser Aufsatz untersucht die Wirksamkeit der t{\"{u}}rkischen Politik, Gastarbeiter{\"{u}}berweisungen aus Deutschland in die T{\"{u}}rkei von 1963 bis 1982 zu f{\"{o}}rdern. Das empirische Modell zeigt, da$\backslash$ solche Bem{\"{u}}hungen nicht sehr erfolgreich waren. Auch Wechselkurs{\"{A}}nderungen (die die Absicht der Regierung widerspiegeln, Fremdw{\"{A}}hrungsdepositen mit Hilfe h{\"{o}}herer Ertr{\"{A}}ge anzulocken) haben den Strom von {\"{u}}berweisungen in die T{\"{u}}rkei nicht beeinflu$\backslash$t. Diese sind weit mehr durch die {\"{o}}konomische Situation in Deutschland bestimmt worden. Die Lohnh{\"{o}}he in Deutschland und die M{\"{o}}glichkeit f{\"{u}}r t{\"{u}}rkische Emigranten, sich dort als Arbeitnehmer zu bet{\"{A}}tigen, haben den Umfang der potentiellen {\"{u}}berweisungen bestimmt. Welcher Teil davon wirklich {\"{u}}berwiesen worden ist, hing von dem Vertrauen der t{\"{u}}rkischen Gastarbeiter in die Sicherheit und Liquidit{\"{A}}t der Depositen in ihrem Heimatland ab. Bei entsprechenden rechtlichen und politischen Rahmenbedingungen in der T{\"{u}}rkei k{\"{o}}nnte die {\"{u}}berweisungsneigung der Emigranten letztlich durch die wirtschaftlichen Anreize bestimmt werden, die eine Investition in der T{\"{u}}rkei lohnender machen als andere Alternativen.},
author = {Straubhaar, Thomas},
journal = {Review of World Economics},
number = {4},
pages = {728--740},
title = {{The determinants of workers' remittances: The case of Turkey}},
url = {http://dx.doi.org/10.1007/BF02707858},
volume = {122},
