journal = {Economics of Transition},
keywords = {Consumption,Credit,Income,Risk-sharing,Russia,Unemployment,Vulnerability,Wage arrears},
number = {1},
pages = {67--91},
title = {{Consumption smoothing in Russia: Evidence from the RLMS}},
volume = {11},
