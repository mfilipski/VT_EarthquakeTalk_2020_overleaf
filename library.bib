journal = {Journal of Neuroscience Methods},
keywords = {alzheimers-disease,arbitrary particles,astrocytosis,brain,ca1,cells,dentate gyrus,expression,mac-1,neurons,optical fractionator,stereology,transgenic mice,unbiased estimation},
language = {English},
number = {1-2},
