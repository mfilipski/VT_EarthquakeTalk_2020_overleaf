@article{You2011,
abstract = {Although irrigation in Africa has the potential to boost agricultural productivities by at least 50{\%}, food production on the continent is almost entirely rainfed. The area equipped for irrigation, currently slightly more than 13 million hectares, makes up just 6{\%} of the total cultivated area. More than 70{\%} of Africa's poor live in rural areas and mostly depend on agriculture for their livelihoods. As a result, agricultural development is key to ending poverty on the continent. Many development organizations have recently proposed to significantly increase investments in irrigation in the region. However, the potential for irrigation investments in Africa is highly dependent upon geographic, hydrologic, agronomic, and economic factors that need to be taken into account when assessing the long-term viability and sustainability of planned projects. This paper analyzes the large, dam-based and small-scale irrigation investment potential in Africa based on agronomic, hydrologic, and economic factors. We find significant profitable irrigation potential for both small-scale and large-scale systems. This type of regional analysis can guide distribution of investment funds across countries and should be a first step prior to in-depth country- and local-level assessment of irrigation potential, which will be important to agricultural and economic development in Africa.},
author = {You, Liangzhi and Ringler, Claudia and Wood-Sichrab, Ulrike and Robertson, Richard and Wood, Stanley and Zhu, Tingju and Nelson, Gerald and Guo, Zhe and Sun, Yan},
file = {:Users/mfilipski/Library/Application Support/Mendeley Desktop/Downloaded/You et al. - 2011 - What is the irrigation potential for Africa A combined biophysical and socioeconomic approach.pdf:pdf},
journal = {Food Policy},
keywords = {Africa,Investment,Irrigation potential,Large-scale irrigation,Small-scale irrigation},
title = {{What is the irrigation potential for Africa? A combined biophysical and socioeconomic approach}},
url = {http://www.sciencedirect.com/science/article/pii/S030691921100114X},
year = {2011}
}
@article{Ohtomo2014,
abstract = {This study examined psychological processes of consumers that had determined hoarding and avoidant purchasing behaviors after the Tohoku earthquake within a dual-process model. The model hypothesized that both intentional motivation based on reflective decision and reactive motivation based on non-reflective decision predicted the behaviors. This study assumed that attitude, subjective norm and descriptive norm in relation to hoarding and avoidant purchasing were determinants of motivations. Residents in the Tokyo metropolitan area (n = 667) completed internet longitudinal surveys at three times (April, June, and November, 2011). The results indicated that intentional and reactive motivation determined avoidant purchasing behaviors in June; only intentional motivation determined the behaviors in November. Attitude was a main determinant of the motivations each time. Moreover, previous behaviors predicted future behaviors. In conclusion, purchasing behaviors were intentional rather than reactive behaviors. Furthermore, attitude and previous behaviors were important determinants in the dual-process model. Attitude and behaviors formed in April continued to strengthen the subsequent decisions of purchasing behavior.},
author = {Ohtomo, Shoji and Hirose, Yukio},
issn = {0021-5236},
journal = {Shinrigaku kenkyu : The Japanese journal of psychology},
keywords = {Adult,Attitude,Choice Behavior,Choice Behavior: physiology,Earthquakes,Female,Humans,Intention,Internet,Longitudinal Studies,Male,Middle Aged,Models, Psychological,Motivation,Motivation: physiology,Obsessive Hoarding,Obsessive Hoarding: psychology,Tokyo},
month = {feb},
