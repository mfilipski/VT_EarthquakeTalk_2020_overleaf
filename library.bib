@misc{Desai2004b,
abstract = {"This paper studies the effects of financial constraints on firm growth by investigating if large depreciations differentially impact multinational affiliates and local firms in emerging markets. U.S. multinational affiliates increase sales, assets and investment significantly more than local firms during, and subsequent to, currency crises. The enhanced relative performance of multinationals is traced to their ability to use internal capital markets to capitalize on the competitiveness benefits of large depreciations. Investment specifications indicate that increases in leverage resulting from sharp depreciations constrain local firms from capitalizing on these investment opportunities, but do not constrain multinational affiliates. Multinational parents also infuse new capital in their affiliates after currency crises. These results indicate another role for foreign direct investment in emerging markets multinational affiliates expand economic activity during currency crises when local firms are most constrained"--NBER website},
address = {Cambridge, Mass.},
annote = {55741934eo{\textless}m:linebreak{\textgreater}{\textless}/m:linebreak{\textgreater}[electronic resource]:{\textless}m:linebreak{\textgreater}{\textless}/m:linebreak{\textgreater}Mihir A. Desai, C. Fritz Foley, Kristin J. Forbes{\textless}m:linebreak{\textgreater}{\textless}/m:linebreak{\textgreater}ill. ; 22 cm{\textless}m:linebreak{\textgreater}{\textless}/m:linebreak{\textgreater}NBER working paper series,{\textless}m:linebreak{\textgreater}{\textless}/m:linebreak{\textgreater}no. 10545{\textless}m:linebreak{\textgreater}{\textless}/m:linebreak{\textgreater}"June 2004."{\textless}m:linebreak{\textgreater}{\textless}/m:linebreak{\textgreater}Includes bibliographical references (p. 26-29){\textless}m:linebreak{\textgreater}{\textless}/m:linebreak{\textgreater}Also issued in print{\textless}m:linebreak{\textgreater}{\textless}/m:linebreak{\textgreater}Electronic reproduction. Mode of access: World Wide Web. Restricted to UC campuses{\textless}m:linebreak{\textgreater}{\textless}/m:linebreak{\textgreater}NBER. Restricted to UCB, UCD, UCI, UCLA, UCM, UCR, UCSB, UCSC, and UCSD},
author = {Desai, Mihir A and Foley, C Fritz and Forbes, Kristin and {National Bureau of Economic Research.}},
