keywords = {a-deficient mice,arylsulfatase a,arylsulfatase-a,brain,differentiate,es cells,expression,gene-transfer,glial precursors,lysosomal storage,metachromatic leukodystrophy,mouse model,oligodendrocytes,spinal-cord transplantation,sulfatide reduction,transplantation},
