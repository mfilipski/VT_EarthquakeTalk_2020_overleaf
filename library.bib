@article{Loschel2002a,
abstract = {This paper provides an overview of the treatment of technological change (TC) in economic models of environmental policy. Numerous economic modeling studies have confirmed the sensitivity of mid- and long-run climate change mitigation cost and benefit projections to assumptions about technology costs. In general, technical progress is considered to be a non-economic, exogenous variable in global climate change modeling. However, there is overwhelming evidence that TC is not an exogenous variable but to an important degree endogenous, induced by needs and pressures. Hence, some energy-economy-environment models treat TC as endogenous, responding to socio-economic variables. Three main elements in models of technological innovation are: (i) corporate investment in research and development (R{\&}D), (ii) spillovers from R{\&}D, and (iii) technology learning, especially learning-by-doing. The incorporation of induced TC in different types of energy-environmental-economic models tends to reduce the costs of environmental policy, accelerates abatement and may lead to positive spillover and negative leakage.},
annote = {From Duplicate 1 ( Technological change in economic models of environmental policy: a survey - Loschel, Andreas )
},
author = {Loschel, Andreas},
file = {:Users/mfilipski/Library/Application Support/Mendeley Desktop/Downloaded/Loschel - 2002 - Technological change in economic models of environmental policy a survey.pdf:pdf},
journal = {Ecological Economics},
keywords = {Energy-economy-environment models,Exogenous technological change,Induced technological change},
number = {2-3},
pages = {105--126},
title = {{Technological change in economic models of environmental policy: a survey}},
url = {http://www.sciencedirect.com/science/article/B6VDY-47C4C8D-2/2/3e6547519e387bd0f0a3ea6d2a438781},
