title = {{Local demand, investment multipliers, and industrialization: Theory and application to the Guatemalan highlands}},
url = {http://www.sciencedirect.com/science/article/B6VBV-3VW1T1S-G/2/23a575601d91a166d73349f9d76e835e},
volume = {49},
year = {1996}
