@article{Christiaensen2005,
abstract = {This study illustrates a methodology to empirically assess household vulnerability using pseudo panel data derived from repeated cross sections augmented with historical information on shocks. It conceives vulnerability as expected poverty. The methodology was applied to data from rural Kenya. To empirically construct a vulnerability profile and examine the determinants of vulnerability, data were used from the 1994 and 1997 Welfare Monitoring Surveys (WMS), complemented with some information on malaria incidence from the 1992 WMS. It is shown that rural households faced in 1994 on average a chance of 39{\%} of becoming poor in the future. Households in arid areas, who experience large rainfall volatility, appear more vulnerable than those in non-arid areas, where malaria emerges as a key risk factor. Idiosyncratic shocks also cause non-negligible consumption volatility. Possession of cattle and sheep/goat appears ineffective in protecting consumption against covariate shocks, though sheep/goat help reduce the effect of idiosyncratic shocks, especially in arid zones. Of the policy instruments simulated, interventions directed at reducing the incidence of malaria, promoting adult literacy, and improving market accessibility hold most promise to reduce vulnerability.},
author = {Christiaensen, Luc and Subbarao, Kalanidhi},
doi = {10.1093/jae/eji008},
isbn = {3326},
issn = {09638024},
journal = {Journal of African Economies},
pages = {520--558},
title = {{Towards an understanding of household vulnerability in rural Kenya}},
volume = {14},
year = {2005}
}
