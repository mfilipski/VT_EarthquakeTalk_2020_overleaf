@article{Robinson2001a,
abstract = {The problem in estimating a social accounting matrix (SAM) for a recent$\backslash$nyear is to find an efficient and cost-effective way to incorporate$\backslash$nand reconcile information from a variety of sources, including data$\backslash$nfrom prior years. Based on information theory, the paper presents$\backslash$na flexible 'cross entropy' (CE) approach to estimating a consistent$\backslash$nSAM starting from inconsistent data estimated with error, a common$\backslash$nexperience in many countries. The method represents an efficient$\backslash$ninformation processing rule-using only and all information available.$\backslash$nIt allows incorporating errors in variables, inequality constraints,$\backslash$nand prior knowledge about any part of the SAM. An example is presented,$\backslash$napplying the CE approach to data from Mozambique, using a Monte Carlo$\backslash$napproach to compare the CE approach to the standard RAS method and$\backslash$nto evaluate the gains in precision from utilizing additional information.},
author = {Robinson, Sherman and Cattaneo, Andrea and El-Said, Moataz},
doi = {10.1080/09535310120026247},
