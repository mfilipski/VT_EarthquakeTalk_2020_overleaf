journal = {Land Use Policy},
keywords = {Agriculture,Forest transition,Land use,Migration,Remittances},
pages = {182--191},
pmid = {24187416},
publisher = {Elsevier Ltd},
title = {{Consequences of out-migration for land use in rural Ecuador}},
