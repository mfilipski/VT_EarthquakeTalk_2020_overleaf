journal = {Health services research},
keywords = {African Americans,Age Factors,Aged,Continental Population Groups,Continental Population Groups: statistics {\&} numeri,Drug Utilization,Drug Utilization: economics,Drug Utilization: statistics {\&} numerical data,European Continental Ancestry Group,Female,Financing,Health Status,Healthcare Disparities,Healthcare Disparities: economics,Healthcare Disparities: ethnology,Hispanic Americans,Humans,Male,Medicare Part D,Medicare Part D: statistics {\&} numerical data,Middle Aged,Personal,Personal: economics,Personal: statistics {\&} numerical data,Prescription Drugs,Prescription Drugs: economics,Sex Factors,United States},
month = {apr},
