@article{Belloni2014,
abstract = {Data with a large number of variables relative to the sample size?$\backslash$"high-dimensional data$\backslash$"?are readily available and increasingly common in empirical economics. High-dimensional data arise through a combination of two phenomena. First, the data may be inherently high dimensional in that many different characteristics per observation are available. For example, the US Census collects information on hundreds of individual characteristics and scanner datasets record transaction-level data for households across a wide range of products. Second, even when the number of available variables is relatively small, researchers rarely know the exact functional form with which the small number of variables enter the model of interest. Researchers are thus faced with a large set of potential variables formed by different ways of interacting and transforming the underlying variables. This paper provides an overview of how innovations in $\backslash$"data mining$\backslash$"? can be adapted and modified to provide high-quality inference about model parameters. Note that we use the term $\backslash$"data mining$\backslash$" in a modern sense which denotes a principled search for $\backslash$"true$\backslash$" predictive power that guards against false discovery and overfitting, does not erroneously equate in-sample fit to out-of-sample predictive ability, and accurately accounts for using the same data to examine many different hypotheses or models.},
author = {Belloni, Alexandre and Chernozhukov, Victor and Hansen, Christian},
doi = {10.1257/jep.28.2.29},
